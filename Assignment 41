//c program to read a string and convert alternative character into upper case.#include<stdio.h>
void main()
{
	char s1[20];
	printf("Enter string");
	gets(s1);
	int i;
	for(i=0;s1[i]!='\0';i++)
	{   if(i%2!=0)
	    s1[i]=s1[i]-32;  
		printf("%c",s1[i]);
     }
    return 0;
 }
 output:
 aditya
 aDiTyA
 
