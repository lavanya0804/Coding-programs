//Write a c program to print given number is armstrong or not.
#include <stdio.h>


int main()

{
   
 int n,r,sum=0,temp;
   
 scanf("%d",&n);
   
 temp=n;
   
 while(n>0)
   
  {
      
     r=n%10;
      
     sum=sum+(r*r*r);
      
     n=n/10;
  
  }
   
  if(sum==temp)
   
  printf("ARMSTRONG");
   
  else
   
  printf("NOT ARMSTRONG");
    
 return 0;

}
output:
153
ARMSTRONG NUMBER
151
NOT ARMSTRONG NUMBER
