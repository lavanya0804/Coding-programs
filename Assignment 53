//C program to read n and m values .print numbers between m and n without using for loop.
#include <stdio.h>
void display();
void main()
{
    int n,m;
    scanf("%d%d",&n,&m);
    display(n,m);
}
void display(int start,int end)
{
    if(start>end)
    return;
    else
    printf("%d ",start);
    display(start+1,end);
}
output:
4   10
4 5 6 7 8 9 10
