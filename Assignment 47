// Digit frequency
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char s[1024];
    int count,i,j;
    char digits[]={'1','2','3','4','5','6','7','8','9'};
    scanf("%s",s);
    for(int i=0;i<10;i++)
    {
        count=0;
        for(j=0;j<strlen(s);j++)
        {
            if(s[j]==digits[i])
            count++;
        }
        printf("%d",count);
        
    }
    return 0;
}
