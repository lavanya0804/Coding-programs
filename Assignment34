#include<stdio.h>
void main()
{
  int i,a[5];
  printf("enter elements into array\n");
  for(i=0;i<5;i++)
  scanf("%d",&a[i]);
  int j,flag;;
  printf("the prime no of arrays\n");
  for(i=0;i<5;i++)
  {
    flag=0;
    for(j=2;j<a[i];j++)
    {
      if{a[i]%j==0)
      {
        flag=1;
        break;
      }
    }
   if(flag==0)
   printf("%d\t",a[i]);
   }
 }  
