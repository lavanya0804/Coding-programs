// C program to print given string is palindrome or not without using string2 & without using string functions. 
#include<stdio.h>
#include<string.h>
void main()
{
	char s[20];
	int i,j,n,count=0;
	printf("Enter string");//madam
	gets(s);
	for(i=0;s[i]!='\0';i++); n=i;  //length of string
	i=0;j=n-1;
	while(i<n/2) //
	{
		if(s[i]==s[j])
		count++;
	  i++;
	  j--;
	}
	if(count==n/2)
	printf("palindrome");
	else
	printf("not palindrome");
}
output:
madam
palindrome
